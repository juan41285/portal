.hexagon {
  position: relative;
  width: 120px; 
  height: 69.28px;
  background-color: #ffffff;
  margin: 34.64px 0;
  box-shadow: 0 0 30px rgba(0,0,0,0.6);
  cursor: pointer
}

.hexagon:before,
.hexagon:after {
  content: "";
  position: absolute;
  z-index: 1;
  width: 84.85px;
  height: 84.85px;
  -webkit-transform: scaleY(0.5774) rotate(-45deg);
  -ms-transform: scaleY(0.5774) rotate(-45deg);
  transform: scaleY(0.5774) rotate(-45deg);
  background-color: inherit;
  left: 17.5736px;
  box-shadow: 0 0 30px rgba(0,0,0,0.6);
}

.hexagon:before {
  top: -42.4264px;
}

.hexagon:after {
  bottom: -42.4264px;
}

/*cover up extra shadows*/
.hexagon span {
  display: block;
  position: absolute;
  top:0px;
  left: 0;
  width:120px;
  height:69.2820px;
  z-index: 2;
  background: inherit;
}

.hexagon:hover{ 
    background-color: #F5F5F5; 
} 

.hexagon:hover:before, .hexagon:hover:after { 
    border-color: #F5F5F5 transparent; 
}  

